<h1>Songs</h1>

<% @songs.each do |song| %>
<h3><%= link_to song.title, song_path(song) %> | <%= song.artist_name %></h3>
  <p>Released: <%= song.release_year %>, <%= song.genre %></p>
  <%= link_to 'View', song %> |
  <%= link_to 'Edit', edit_song_path(song) %> |
  <%= link_to 'Delete', song, method: :delete, data: { confirm: 'For real?' } %>
<% end %>

<h4><%= link_to 'Create Song', new_song_path %></h4>